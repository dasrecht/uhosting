[uwsgi]
plugins = <%= @plugins %>
chown-socket = www-data:www-data
uid = <%= @name %>
gid = <%= @name %>
socket = /run/uwsgi/<%= @name %>.socket
stats = /run/uwsgi/<%= @name %>.stats.socket
logto = /var/log/uwsgi/<%= @name %>.log

<%- if @vassal_params then @vassal_params.each do |k,v| -%>
  <%- if v != '' -%>
<%= k %> = <%= v %>
  <%- end -%>
<%- end end -%>
