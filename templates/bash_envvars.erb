<%- @_env_vars.each do |k,v| -%>
export <%= k.upcase %>="<%= v %>"
<%- end -%>
<% if @sitedata['stack_type'] == 'phpfpm' or @sitedata['stack_type'] == 'nodejs'  %>
alias start='sudo supervisorctl start <%= @sitedata['stack_type'] %>-<%= @name %>'
alias stop='sudo supervisorctl stop <%= @sitedata['stack_type'] %>-<%= @name %>'
alias restart='sudo supervisorctl restart <%= @sitedata['stack_type'] %>-<%= @name %>'
<% end %>
